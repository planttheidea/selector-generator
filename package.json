{
  "author": "planttheidea <tony.quetano@planttheidea.com>",
  "bugs": {
    "url": "https://github.com/planttheidea/selectorator/issues"
  },
  "dependencies": {
    "fast-equals": "^1.2.1",
    "identitate": "^1.0.0",
    "reselect": "^4.0.0",
    "unchanged": "^1.0.7"
  },
  "description": "Simplified generator of reselect selectors",
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "cli-table2": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "in-publish": "^2.0.0",
    "jest": "^23.6.0",
    "moize": "^5.4.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "rollup": "^0.66.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "homepage": "https://github.com/planttheidea/selectorator#readme",
  "keywords": [
    "reselect",
    "selector"
  ],
  "license": "MIT",
  "main": "dist/selectorator.js",
  "name": "selectorator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/selectorator.git"
  },
  "scripts": {
    "benchmark": "rimraf lib && npm run transpile:lib && node ./benchmark/index.js",
    "build": "NODE_ENV=production rollup -c",
    "build:types": "cp src/types.ts dist/index.d.ts",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development webpack-dev-server --progress --colors --config=webpack/webpack.config.js",
    "lint": "NODE_ENV=test tslint 'src/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "if in-publish; then npm run prepublish:compile; fi",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run clean && npm run build && npm run build:types",
    "start": "npm run dev",
    "test": "NODE_PATH=. jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "version": "3.3.0"
}
